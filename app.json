{
  "expo": {
    "name": "Azura App",
    "slug": "azura-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_icon.png",
    "scheme": "azuraapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.malakstar786.azuraapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./src/assets/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/icon.png",
          "imageWidth": 300,
          "resizeMode": "native",
          "backgroundColor": "#000000",
          "dark": {
            "image": "./src/assets/icon.png",
            "imageWidth": 300,
            "resizeMode": "native",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "apple-pay-react-native-expo",
        {
          "merchantIdentifiers": ["merchant.kw.com.azura"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0e6237c0-8566-4930-9a17-06692987195d"
      }
    },
    "owner": "malakstar786",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0e6237c0-8566-4930-9a17-06692987195d"
    }
  }
}
